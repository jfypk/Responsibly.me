{
    "manifest_version": 2,
    "name": "Responsibly.me",
    "description": "How responsible are the companies you shop from?",
    "version": "0.0.1",
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Shop Responsibly"
    },
    "icons": {
        "16": "favicon.png",
        "48": "favicon.png",
        "128": "favicon.png"
    },
    "permissions": ["tabs"]
}