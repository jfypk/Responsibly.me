{
    "manifest_version": 2,
    "name": "Inform Extension",
    "version": "0.1",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["inform-ext-content.js"]
    }],
    "permissions": [],
    "browser_action": {
        "default_icon": "tree.png",
        "default_popup": "inform-popup.html",
        "default_title": "Learn the social responsibility initiatives of this company"
    }
}